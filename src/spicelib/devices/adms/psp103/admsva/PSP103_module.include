        // Transistor temperature
        TR_i        = `CLIP_LOW(TR, -273.0);
        TKR         = `KELVINCONVERSION + TR_i;
        TKA         =  $temperature + DTA;
        rTa         =  TKA / TKR;
        delTa       =  TKA - TKR;
        phita       =  TKA * `KBOL / `QELE;
        inv_phita   =  1.0 / phita;
